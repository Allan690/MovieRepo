{
  "name": "movies",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "ts-node -r tsconfig-paths/register ",
    "prebuild": "yarn clean",
    "build": "yarn run clean && yarn run build-server",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -c",
    "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "build-server": "tsc -b tsconfig.build.json",
    "drop:db": "ts-node ./node_modules/typeorm/cli.js schema:drop -c",
    "rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert -c",
    "roll:migrate": "yarn rollback development && yarn migrate",
    "test": "jest --no-cache  --detectOpenHandles --runInBand --forceExit",
    "start:dev": "NODE_ENV=development DEBUG=log nodemon",
    "start": "yarn build && DEBUG=log ts-node dist/index.js -p $PORT",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "yarn lint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "dotenv-extended": "^2.8.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "ts-node": "^9.0.0",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/nodemon": "^1.19.0",
    "nodemon": "^2.0.4",
    "tsconfig-paths": "^3.9.0"
  }
}
